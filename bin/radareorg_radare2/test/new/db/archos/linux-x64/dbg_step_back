NAME='dbg.stepback'
FILE=../bins/elf/analysis/ls-linux-x86_64-zlul
ARGS=-d
CMDS=<<EXPECT
db main
db 0x004028fe
dc
dts+
dc
dsb
dr rip
dk 9
EXPECT=<<RUN
0x004028f9
RUN

NAME='debug stepback from callee'
FILE=../bins/elf/analysis/calls_x64
ARGS=-d
BROKEN=1
CMDS=<<EXPECT
db main
db 0x0040052f
dc
dts+
dc
dsb
dr rip
dk 9
EXPECT=<<RUN
0x00400575
RUN

NAME='debug stepback from caller'
FILE=../bins/elf/analysis/calls_x64
ARGS=-d
BROKEN=1
CMDS=<<EXPECT
db main
db 0x0040057c
dc
dts+
dc
dsb
dr rip
EXPECT=<<RUN
0x0040053b
Hello world
RUN
