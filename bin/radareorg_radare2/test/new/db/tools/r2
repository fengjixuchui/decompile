NAME=r2 -
FILE=-
CMDS=e file.path
EXPECT=<<EOF

EOF
RUN

NAME=r2 -
FILE=-
CMDS=e file.lastpath
EXPECT=<<EOF

EOF
RUN

NAME=r2 --
FILE=--
CMDS=e file.path
EXPECT=<<EOF

EOF
RUN

NAME=r2 --
FILE=../bins/elf/bomb
BROKEN=1
CMDS=r2 -s main -qcs ../bins/elf/bomb
EXPECT=<<EOF
0x400da0
EOF
RUN

NAME=seek undo/redo
FILE=-
CMDS=<<EXPECT
wx 01
s 1
s-
s+
pb 8
EXPECT=<<EOF
00000000
EOF
RUN

NAME=seek undo/redo
FILE=-
CMDS=wx 01; s 1; s-; s+; pb 8
EXPECT=<<EOF
00000000
EOF
RUN

NAME=r2 -V
BROKEN=1 # Only at Windows
FILE=-
CMDS=!!r2 -V~r_anal?
EXPECT=<<EOF
1
EOF
RUN

NAME=r2 -h
BROKEN=1 # Only at Windows
FILE=-
CMDS=!!r2 -h~Usage?
EXPECT=<<EOF
1
EOF
RUN

NAME=r2 -hh
BROKEN=1 # Only at Windows
FILE=-
CMDS=!!r2 -hh~R2_USER_PLUGINS?
EXPECT=<<EOF
1
EOF
RUN

NAME=r2 -L
BROKEN=1 # Only at Windows
FILE=-
CMDS=!!r2 -L~default?
EXPECT=<<EOF
1
EOF
RUN

NAME=ONE_STREAM sets scr.onestream
FILE=-
ONE_STREAM=1
CMDS=e scr.onestream
EXPECT=true
RUN
