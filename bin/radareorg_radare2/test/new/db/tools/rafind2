BROKEN=1
FILE=-
CMDS=!!rafind2 -m ../bins/elf/ioli/crackme0x00
EXPECT=<<EOF
0x00000000 1 ELF 32-bit LSB executable, Intel 80386, version 1
EOF
RUN

