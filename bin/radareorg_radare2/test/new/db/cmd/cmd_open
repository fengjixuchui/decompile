NAME=oob 10
FILE=../bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
xor ebp, ebp
EOF
CMDS=<<EOF
10oob
pi 1
EOF
RUN

NAME=oo+ 10
FILE=../bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
xor ebp, ebp
EOF
CMDS=<<EOF
10oo+
pi 1
EOF
RUN

NAME=oob consider baddr
FILE=../bins/mach0/mac-ls
CMDS=<<EXPECT
e bin.baddr = 0xf00000
k old_v=`ieq`
oob
iI~baddr[1]
ie~vaddr[1]
p8 10 @ entry0
p8 10 @ `k old_v`
EXPECT=<<RUN
0xf00000
0x00f01058
554889e5415741564155
ffffffffffffffffffff
RUN

NAME=oob consider laddr 32bit
FILE=../bins/mach0/fatmach0-3true
ARGS=-a x86 -b 32
CMDS=<<EXPECT
e bin.laddr = 0x5000
e bin.baddr = -1
oob
iI~laddr[1]
iI~baddr[1]
EXPECT=<<RUN
0x0
0x1000
RUN

NAME=oob consider laddr 64bit
FILE=../bins/mach0/fatmach0-3true
ARGS=-a x86 -b 64
CMDS=<<EXPECT
e bin.laddr = 0x5000
e bin.baddr = -1
oob
iI~laddr[1]
iI~baddr[1]
EXPECT=<<RUN
0x0
0x100000000
RUN

NAME=oob from malloc
FILE=malloc://1024
EXPECT=<<EOF
elf
b32ae931000000000200
EOF
CMDS=<<EOF
wx 
oba
i~bintype[1]
.ie*
p8 10 @ entry0
EOF
RUN

NAME=ob select files
FILE=malloc://1024
CMDS=<<EXPECT
e scr.null=true
o malloc://512
e scr.null=false
i~file[1]
obo 3
i~file[1]
obo 4
i~file[1]
obo 3
i~file[1]
EXPECT=<<RUN
malloc://512
malloc://1024
malloc://512
malloc://1024
RUN

