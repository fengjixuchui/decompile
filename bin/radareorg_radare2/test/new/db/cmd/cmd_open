o malloc://1024
e scr.null=false
iiq~?
EOF
RUN

NAME=ob select files binobj2
FILE=../bins/elf/libmagic.so
EXPECT=<<EOF
408
46
408
EOF
CMDS=<<EOF
isq~?
e scr.null=true
o ../bins/elf/true32
e scr.null=false
isq~?
# raise back
op `o~:0[0]`
isq~?
EOF
RUN

