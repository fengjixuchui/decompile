NAME=cmd.hit sugar for /x
FILE=malloc://1024
EXPECT=<<EOF
0x0000000a hit0_0 90
91
EOF
CMDS=<<EOF
wx 90 @ 10
e cmd.hit=wx 91
/x 90
p8 1 @ 10
EOF
RUN

