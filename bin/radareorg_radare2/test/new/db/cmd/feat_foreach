NAME=@@= 10 20 30
FILE=-
EXPECT=<<EOF
0xa
0x14
0x1e
EOF
CMDS=<<EOF
?v $$ @@= 10 20 30
EOF
RUN

NAME=@@c:/c fs:
FILE=../bins/mach0/mac-ls
EXPECT=<<EOF
0x100005078
EOF
CMDS=<<EOF
e search.in = bin.sections
?v $$ @@c:/ad fs:[ra
EOF
RUN

NAME=@@c:/c/a fs:
FILE=../bins/mach0/mac-ls
EXPECT=<<EOF
0x100004cd7
0x100004d95
0x100004e1d
0x100005078
EOF
CMDS=<<EOF
e search.in = bin.sections
?v $$ @@c:/ao fs:[ra
EOF
RUN

NAME=@@= 10   20   30
FILE=-
EXPECT=<<EOF
0xa
0x14
0x1e
EOF
CMDS=<<EOF
?v $$ @@= 10   20   30
EOF
RUN

NAME=@@= `prz` fix
FILE=-
ONE_STREAM=1
CMDS=<<EXPECT
wz 1 2 3 4
pd 1 @@= `prz`
EXPECT=<<RUN
            0x00000001      2032           and byte [rdx], dh
            0x00000002      3220           xor ah, byte [rax]
            0x00000003      2033           and byte [rbx], dh
            0x00000004      3320           xor esp, dword [rax]
RUN
