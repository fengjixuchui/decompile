NAME=PE: corkami delayfake.exe - open
FILE=../bins/pe/delayfake.exe
EXPECT=<<EOF
EOF
CMDS=<<EOF
q!
EOF
RUN

NAME=PE: corkami delayfake.exe - entrypoint
FILE=../bins/pe/delayfake.exe
EXPECT=<<EOF
0x401000
EOF
CMDS=<<EOF
s
EOF
RUN

NAME=PE: corkami delayfake.exe - pi 1
FILE=../bins/pe/delayfake.exe
EXPECT=<<EOF
push 0x401018
EOF
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
RUN
