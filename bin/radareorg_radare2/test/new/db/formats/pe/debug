NAME=PE: corkami debug.exe - open
FILE=../bins/pe/debug.exe
EXPECT=<<EOF
EOF
CMDS=<<EOF
q!
EOF
RUN

NAME=PE: corkami debug.exe - entrypoint
FILE=../bins/pe/debug.exe
EXPECT=<<EOF
0x401000
EOF
CMDS=<<EOF
s
EOF
RUN

NAME=PE: corkami debug.exe - pi 1
FILE=../bins/pe/debug.exe
EXPECT=<<EOF
push 0
EOF
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
RUN
