NAME=PE: corkami ctxt.dll - open
FILE=../bins/pe/ctxt.dll
EXPECT=<<EOF
EOF
CMDS=<<EOF
q!
EOF
RUN

NAME=PE: corkami ctxt.dll - entrypoint
FILE=../bins/pe/ctxt.dll
EXPECT=<<EOF
0x1001000
EOF
CMDS=<<EOF
s
EOF
RUN

NAME=PE: corkami ctxt.dll - entrypoint
FILE=../bins/pe/ctxt.dll
EXPECT=<<EOF
0x1001000
EOF
CMDS=<<EOF
s
EOF
RUN

NAME=PE: corkami ctxt.dll - pi 1
FILE=../bins/pe/ctxt.dll
EXPECT=<<EOF
cmp dword [esp + 8], 1
EOF
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
RUN
