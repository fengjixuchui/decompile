NAME=PE: corkami dep.exe - open
FILE=../bins/pe/dep.exe
EXPECT=<<EOF
EOF
CMDS=<<EOF
q!
EOF
RUN

NAME=1: PE: corkami dep.exe - dep not enabled
FILE=../bins/pe/no_dep.exe
ARGS=-n
EXPECT=<<EOF
false

EOF
CMDS=<<EOF
!rabin2 -k pe/nx ../bins/pe/no_dep.exe
EOF
RUN

NAME=2: PE: corkami dep.exe - dep not enabled
FILE=../bins/pe/no_dep.exe
EXPECT=<<EOF
false
EOF
CMDS=<<EOF
k bin/cur/pe/nx
EOF
RUN
