NAME=PE: corkami dll-ld.exe - open
FILE=../bins/pe/dll-ld.exe
EXPECT=<<EOF
EOF
CMDS=<<EOF
q!
EOF
RUN

NAME=PE: corkami dll-ld.exe - entrypoint
FILE=../bins/pe/dll-ld.exe
EXPECT=<<EOF
0x401000
EOF
CMDS=<<EOF
s
EOF
RUN

NAME=PE: corkami dll-ld.exe - pi 1
FILE=../bins/pe/dll-ld.exe
EXPECT=<<EOF
call dword [0x4010a0]
EOF
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
RUN
