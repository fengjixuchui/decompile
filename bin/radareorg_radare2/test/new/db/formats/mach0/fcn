NAME=anal fcn names with symbols
FILE=../bins/mach0/libr_bp.dylib
ARGS=-A
EXPECT=<<EOF
0x00000760    7 379          sym._r_bp_new
EOF
CMDS=<<EOF
afl~00760
EOF
RUN

NAME=anal single fcn
FILE=../bins/mach0/libr_bp.dylib
EXPECT=<<EOF
name: sym._r_bp_new
EOF
CMDS=<<EOF
s 0x760
af
afi~name
EOF
RUN

NAME=anal main fcn
FILE=../bins/mach0/ls-osx-x86_64
EXPECT=<<EOF
name: main
EOF
CMDS=<<EOF
af
afi~name
EOF
RUN
