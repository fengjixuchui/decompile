NAME=fatmach0 string location
FILE=../bins/mach0/BatteryLife
EXPECT=<<EOF
batteryLevel
EOF
CMDS=<<EOF
s str.batteryLevel
ps
EOF
RUN

NAME=fatmach0 -b64
FILE=../bins/mach0/BatteryLife
ARGS=-aarm -b64
EXPECT=<<EOF
64
  0x100010f7c      stp x22, x21, [sp, -0x30]!
EOF
CMDS=<<EOF
e asm.bits
e asm.bytes=0
e asm.flags=0
e asm.lines.bb=0
pd 1
EOF
RUN

