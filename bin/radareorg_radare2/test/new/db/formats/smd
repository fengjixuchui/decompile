NAME=sega megadrive rom info
FILE=../bins/smd/LiquidSpaceDodgerV3.bin
EXPECT=<<EOF
file     ../bins/smd/LiquidSpaceDodgerV3.bin
size     0x272f8
humansz  156.7K
mode     r-x
format   smd
iorw     false
blksz    0x0
block    0x100
type     ROM
arch     m68k
baddr    0x0
binsz    160504
bits     16
canary   false
retguard false
class    SEGA MEGASIS    (C)2015 James L.
crypto   false
endian   big
havecode true
laddr    0x0
linenum  false
lsyms    false
machine  Sega Megadrive
nx       false
os       smd
pic      false
relocs   false
sanitiz  false
static   true
stripped false
va       true
EOF
CMDS=<<EOF
i~!fd
EOF
RUN

