NAME=avr entrypoint
FILE=../bins/elf/analysis/bugurtos-avr.elf
CMDS=ie~program
EXPECT=<<EOF
vaddr=0x00000000 paddr=0x00000074 haddr=0x00000018 hvaddr=0x00000018 type=program
EOF
RUN

NAME=avr baddr
FILE=../bins/elf/analysis/bugurtos-avr.elf
CMDS=iI~baddr[1]
EXPECT=<<EOF
0x0
EOF
RUN

NAME=avr main address
FILE=../bins/elf/analysis/bugurtos-avr.elf
EXPECT=<<EOF
0x308
EOF
CMDS=<<EOF
?v sym.main
EOF
RUN

NAME=avr use va
FILE=../bins/elf/analysis/bugurtos-avr.elf
EXPECT=<<EOF
0c9446000c9430060c94
ef92ff920f93df93cf93
EOF
CMDS=<<EOF
p8 10 @ entry0
p8 10 @ sym.main
EOF
RUN

NAME=avr main code
FILE=../bins/elf/analysis/bugurtos-avr.elf
EXPECT=<<EOF
push r14
push r15
push r16
EOF
CMDS=<<EOF
pi 3 @ sym.main
EOF
RUN
